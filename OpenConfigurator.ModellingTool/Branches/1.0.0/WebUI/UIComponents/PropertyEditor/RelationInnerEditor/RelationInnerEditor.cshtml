@inherits System.Web.Mvc.WebViewPage
<div>
    <div class="PropertyDiv">
        <span class="label-Small">Type:</span>
        <select class="Dropdown" data-bind="value: RelationType">
            <option value="0">Mandatory</option>
            <option value="1">Optional</option>
            <option value="2">Cloneable</option>
        </select>
    </div>
    <div class="PropertyDiv">
        <span class="label-Small">Identifier:</span>
        <input class="Textbox" type="text" data-bind="value: Identifier, valueUpdate: 'afterkeydown'"/>
    </div>
    <div style="clear:both"></div>
</div>